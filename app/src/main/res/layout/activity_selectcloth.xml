<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".SelectclothActivity">

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="214dp"
        android:layout_height="160dp"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="50dp"
        android:onClick="processsellclothLinkCLick"
        app:srcCompat="@drawable/sell"
        tools:ignore="OnClick" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="224dp"
        android:layout_height="167dp"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="50dp"
        android:onClick="processbuyclothLinkCLick"
        app:srcCompat="@drawable/buy" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="252dp"
        android:layout_height="122dp"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="30dp"
        android:onClick="processdonateclothLinkCLick"
        app:srcCompat="@drawable/donate" />
</LinearLayout>



